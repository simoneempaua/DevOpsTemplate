<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MonthlyPriceAuto__c</fullName>
    <description>Calculates Hourly Rate * Monthly Allowance for Managed Service.</description>
    <externalId>false</externalId>
    <formula>IF( !ISNULL(MonthlyAllowance__c) &amp;&amp; !ISNULL(HourlyRate__c),
MonthlyAllowance__c * HourlyRate__c,
NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculates Hourly Rate * Monthly Allowance for Managed Service.</inlineHelpText>
    <label>Monthly Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
